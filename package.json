    "axios": "^0.19.0",
    "cron-parser": "^2.13.0",
    "cronstrue": "^1.86.0",
    "file-saver": "^2.0.2",
    "fuzzy": "^0.1.3",
    "humanize-duration": "^3.21.0",
    "jaeger-react-trace-component": "^1.0.16",
    "lodash": "^4.17.15",
    "markdown-it": "^10.0.0",
    "moment": "^2.24.0",
    "object.fromentries": "^2.0.1",
