    "react-copy-to-clipboard": "^5.0.2",
    "react-dom": "^16.12.0",
    "react-graph-vis": "^1.0.5",
    "react-joyride": "^2.1.1",
    "react-json-view": "^1.19.1",
    "react-markdown-editor-lite": "^0.5.0",
    "react-moment": "^0.9.2",
    "react-particles-js": "^2.7.0",
