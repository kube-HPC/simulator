{
  "name": "simulator",
  "version": "2.11.15",
  "description": "The Hkube Dashboard",
  "private": true,
  "homepage": ".",
  "type": "module",
  "scripts": {
    "analyze": "npm run builder && source-map-explorer 'build/static/*.js'",
    "start": "cross-env VITE_VERSION=$npm_package_version vite",
    "start:proxy": "./dev-proxy/run.sh",
    "builder": "cross-env VITE_VERSION=$npm_package_version NODE_OPTIONS='--max-old-space-size=8192' vite build",
    "preview": "vite preview",
    "build": "./dockerfile/build.sh simulator",
    "test-travis": "echo \"No test specified, in future we may be use vitest run\" && exit 0",
    "start:server": "node ./server/server.mjs",
    "prettify": "prettier --write src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "lint": "eslint src/**/*.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not ie <= 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
