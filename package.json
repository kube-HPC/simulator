{
  "name": "simulator",
  "version": "1.2.1",
  "private": true,
  "homepage": ".",
  "dependencies": {
    "@nivo/bar": "^0.59.2",
    "antd": "^3.21.4",
    "axios": "^0.19.0",
    "cron-parser": "^2.13.0",
    "cronstrue": "^1.82.0",
    "file-saver": "^2.0.2",
    "humanize-duration": "^3.20.1",
    "jaeger-react-trace-component": "^1.0.16",
    "lodash": "^4.17.15",
    "moment": "^2.22.2",
    "monaco-editor-webpack-plugin": "^1.7.0",
    "parse-unit": "^1.0.1",
    "prop-types": "^15.6.2",
    "react": "^16.9.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.9.0",
    "react-graph-vis": "^1.0.2",
    "react-joyride": "^2.1.1",
    "react-json-view": "^1.19.1",
    "react-markdown": "^4.1.0",
    "react-moment": "^0.9.2",
    "react-monaco-editor": "^0.25.1",
    "react-particles-js": "^2.7.0",
    "react-redux": "^7.1.0",
    "react-scripts": "^3.1.1",
    "react-simplemde-editor": "^4.1.0",
    "react-spring": "^8.0.27",
    "redux": "^4.0.4",
    "redux-actions": "^2.6.5",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.1.1",
    "reselect": "^4.0.0",
    "seamless-immutable": "^7.1.4",
    "socket.io-client": "^2.2.0",
    "styled-components": "^5.0.0-beta.6-ej4"
  },
  "scripts": {
    "analyze": "npm run build; source-map-explorer 'build/static/js/*.js'",
    "start": "REACT_APP_VERSION=$npm_package_version PORT=9050 react-app-rewired start",
    "builder": "REACT_APP_VERSION=$npm_package_version react-app-rewired --max_old_space_size=4096 build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "build": "./dockerfile/build.sh simulator",
    "test-travis": "echo \"No test specified\"",
    "start:server": "node ./server/server.js",
    "storybook": "NODE_PATH=src/ start-storybook --port 9001",
    "format": "prettier --write src/**/*.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "eslint:recommended",
      "plugin:react/recommended",
      "prettier/react"
    ],
    "plugins": [
      "react-hooks",
      "jsx-a11y",
      "flowtype"
    ],
    "rules": {
      "react/display-name": 0
    }
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not ie <= 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.1.11",
    "@storybook/addon-links": "^5.1.11",
    "@storybook/addons": "^5.1.11",
    "@storybook/react": "^5.1.11",
    "@welldone-software/why-did-you-render": "^3.3.3",
    "babel-cli": "^6.26.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-import": "^1.12.0",
    "core-js": "^3.2.1",
    "eslint-config-prettier": "^6.1.0",
    "http-proxy-middleware": "^0.19.1",
    "husky": "^3.0.4",
    "lint-staged": "^9.2.3",
    "prettier": "1.18.2",
    "prettier-eslint": "^9.0.0",
    "react-app-rewired": "^2.1.3",
    "source-map-explorer": "^2.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,json,md}": [
      "prettier --write src/**/*.js",
      "git add"
    ]
  }
}
